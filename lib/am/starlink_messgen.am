# Compile messages for message file %MSG%.  This requires that the 
# @MESSGEN@ substitution was requested in configure.ac, probably implicitly
# by the STAR_MESSGEN macro.
%UFAC%_ERR: %MSG%
	@MESSGEN@ -F %MSG%
%LFAC%_err: %MSG%
	@MESSGEN@ -f %MSG%
%LFAC%_err.h: %MSG%
	@MESSGEN@ -c %MSG%
fac_%CODE%_err: %MSG%
	@MESSGEN@ -e %MSG%


