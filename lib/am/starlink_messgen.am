# Compile messages for message file %MSG%
%UFAC%_ERR: %MSG%
	$(STARLINK)/bin/messgen -F %MSG%
%LFAC%_err: %MSG%
	$(STARLINK)/bin/messgen -f %MSG%
%LFAC%_err.h: %MSG%
	$(STARLINK)/bin/messgen -c %MSG%
fac_%CODE%_err: %MSG%
	$(STARLINK)/bin/messgen -e %MSG%


