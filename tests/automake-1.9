  # is contrary to our usage.  Disable this feature.
  alias -g '${1+"$@"}'='"$@"'
fi

perllibdir="/home/norman/Scratch/s/cvs.starlink.ac.uk/trunk/thirdparty/fsf/automake/tests/.././lib:/home/norman/Scratch/s/cvs.starlink.ac.uk/trunk/thirdparty/fsf/automake/tests/../lib"
export perllibdir
exec /home/norman/Scratch/s/cvs.starlink.ac.uk/trunk/thirdparty/fsf/automake/tests/.././automake --libdir=/home/norman/Scratch/s/cvs.starlink.ac.uk/trunk/thirdparty/fsf/automake/tests/../lib ${1+"$@"}
